<Window x:Class="Lab_3.AddAircraftWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab_3"
        mc:Ignorable="d"
        Title="AddAircraftWindow" Height="200" Width="250">
    <Grid>
        <StackPanel HorizontalAlignment="Center" Orientation="Vertical" VerticalAlignment="Center" Margin="0,20,0,20">
            <Label Content="Registration code"/>
            <TextBox x:Name="TextBoxRegCode" TextWrapping="Wrap" Width="120" Margin="0,10,0,10"/>
            <Button x:Name="ButtonSubmit" Content="Submit" Click="ButtonSubmit_Click"/>

        </StackPanel>

    </Grid>
</Window>
